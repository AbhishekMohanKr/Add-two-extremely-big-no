from itertools import zip_longest


def add(a, b):
    out = []
    a = [int(c) for c in str(a)]
    b = [int(c) for c in str(b)]
    carry = 0
    for ca, cb in zip_longest(reversed(a), reversed(b), fillvalue=0):
        carry, digit = divmod(ca + cb + carry, 10)
        out.append(str(digit))
    return "".join(reversed(out)).lstrip("0")


a = 99999299292937363536373736363737327827363636373737373783836456373738383736363636363737738383838373737736373636363637372828383763636373728282828821919299292833838477446667337828292910183383757728282891010
b = 32483836373737337733746632782811901019293833848474777474838328291001928338384747747473838229921001102029833838377477473732882292902022938384847477747474747383822892290210010192833873477746564447737382828
add(a, b)
